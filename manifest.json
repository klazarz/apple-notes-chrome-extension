{
  "manifest_version": 3,
  "name": "Save to Apple Notes",
  "key": "",
  "version": "1.0",
  "description": "Save the current webpage to Apple Notes with one click",
  "permissions": [
    "activeTab",
    "nativeMessaging"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
